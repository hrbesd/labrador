function tapTR1(targetTable,targetTr)
{
    for (i = 0; i < targetTable.rows.length; i++)
    {
        if(targetTr!=i)
        {
            targetTable.rows[i].style.display = "none";
            document.getElementById("me"+i).className = "";
        }
        else
        {
            document.getElementById("me"+i).className = "a1";
            targetTable.rows[i].style.display = "block";
        }
    }
}
